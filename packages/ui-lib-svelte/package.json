{
  "name": "@packages/ui-lib-svelte",
  "version": "0.0.0",
  "sideEffects": [
    "**/*.css"
  ],
  "type": "module",
  "exports": {
    "./components/ui/*": {
      "types": "./src/lib/components/ui/*/index.ts",
      "svelte": "./src/lib/components/ui/*/index.ts"
    },
    "./styles/*": "./src/styles/*"
  },
  "scripts": {
    "build": "svelte-kit sync",
    "clean": "del .turbo coverage .svelte-kit",
    "dev": "vite dev",
    "lint": "eslint .",
    "test": "vitest run",
    "typecheck": "svelte-check --tsconfig ./tsconfig.json"
  },
  "devDependencies": {
    "@lucide/svelte": "0.535.0",
    "@sveltejs/adapter-auto": "6.0.1",
    "@sveltejs/kit": "2.27.0",
    "@sveltejs/vite-plugin-svelte": "6.1.0",
    "@tailwindcss/vite": "4.1.11",
    "@toolchain/eslint-config": "workspace:*",
    "@toolchain/typescript-config": "workspace:*",
    "@toolchain/vitest-config": "workspace:*",
    "bits-ui": "2.9.0",
    "clsx": "2.1.1",
    "mode-watcher": "1.1.0",
    "svelte": "5.37.2",
    "svelte-check": "4.3.0",
    "svelte-sonner": "1.0.5",
    "tailwind-merge": "3.3.1",
    "tailwind-variants": "2.1.0",
    "tailwindcss": "4.1.11",
    "tw-animate-css": "1.3.6",
    "vite": "7.0.6"
  }
}
