{
  "name": "@packages/ui-lib-svelte",
  "version": "0.0.0",
  "sideEffects": [
    "**/*.css"
  ],
  "type": "module",
  "exports": {
    "./components/ui/*": {
      "types": "./src/lib/components/ui/*/index.ts",
      "svelte": "./src/lib/components/ui/*/index.ts"
    },
    "./styles/*": "./src/styles/*"
  },
  "scripts": {
    "build": "svelte-kit sync",
    "clean": "del .turbo coverage .svelte-kit",
    "dev": "vite dev",
    "lint": "eslint .",
    "test": "vitest run",
    "typecheck": "svelte-check --tsconfig ./tsconfig.json"
  },
  "dependencies": {
    "@lucide/svelte": "0.542.0",
    "bits-ui": "2.9.6",
    "svelte-sonner": "1.0.5"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "6.1.0",
    "@sveltejs/kit": "2.37.0",
    "@sveltejs/vite-plugin-svelte": "6.1.3",
    "@tailwindcss/forms": "0.5.10",
    "@tailwindcss/vite": "4.1.12",
    "@toolchain/eslint-config": "workspace:*",
    "@toolchain/typescript-config": "workspace:*",
    "@toolchain/vitest-config": "workspace:*",
    "clsx": "2.1.1",
    "mode-watcher": "1.1.0",
    "svelte": "5.38.6",
    "svelte-check": "4.3.1",
    "tailwind-merge": "3.3.1",
    "tailwind-variants": "3.1.0",
    "tailwindcss": "4.1.12",
    "tw-animate-css": "1.3.7",
    "vite": "7.1.4"
  }
}
