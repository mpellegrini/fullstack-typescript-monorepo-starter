{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"]
    },
    "clean": {
      "cache": false
    },
    "codegen": {},
    "lint": {
      "dependsOn": ["codegen"]
    },
    "test": {
      "outputs": ["./coverage/**"]
    },
    "typecheck": {
      "dependsOn": ["codegen"]
    }
  }
}
