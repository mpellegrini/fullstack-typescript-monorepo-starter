{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"]
    },
    "clean": {
      "cache": false
    },
    "code-gen": {},
    "lint": {
      "dependsOn": ["code-gen"]
    },
    "test": {
      "outputs": ["./coverage/**"]
    },
    "type-check": {
      "dependsOn": ["code-gen"]
    }
  }
}
